{% extends 'base.html.twig' %}

{% block title %}TypeDepense index{% endblock %}

{% block body %}
    <h1>TypeDepense index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_depense in type_depenses %}
            <tr>
                <td>{{ type_depense.id }}</td>
                <td>{{ type_depense.nom }}</td>
                <td>
                    <a href="{{ path('app_type_depense_show', {'id': type_depense.id}) }}">show</a>
                    <a href="{{ path('app_type_depense_edit', {'id': type_depense.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_type_depense_new') }}">Create new</a>
{% endblock %}
